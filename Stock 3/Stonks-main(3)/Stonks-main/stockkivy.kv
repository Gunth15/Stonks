WindowManager:
    SubmitTicker:
    GraphDisplays:

<SubmitTicker>
    name:"StockTickerEntry"
    GridLayout:
        cols:1
        GridLayout:
            cols:2
            Label:
                text: "Type Ticker Here:"
            TextInput:
                id: tickerkivyinput
                multiline: False
        Button:
            text: "Submit"
            on_press: on_press: app.clicked(tickerkivyinput.text)
            on_release: app.root.current = "DisplayTerminal"

<GraphDisplays>
    name:"DisplayTerminal"

    GridLayout:
        cols:1
        Image:
            source: 'debtequityratiograph.jpg'
            size: self.texture_size
        
        Button:
            text:"Go Back"
            on_release: app.root.current = "StockTickerEntry"
